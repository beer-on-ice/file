    "@typescript-eslint/parser": "^4.33.0",
    "commitizen": "^4.2.4",
    "commitlint": "^15.0.0",
    "compare-func": "^2.0.0",
    "compressing": "^1.5.1",
    "conventional-changelog-cli": "^2.0.28",
    "conventional-changelog-cmyr-config": "^2.0.0",
    "conventional-github-releaser": "^3.1.3",
    "conventional-recommended-bump": "^6.1.0",
    "cross-env": "^7.0.2",
