    "gitmoji-cli": "^4.7.0",
    "gulp": "^4.0.2",
    "husky": "^7.0.4",
    "install": "^0.13.0",
    "lint-staged": "^12.1.3",
    "release-it": "^14.11.8",
    "rimraf": "^3.0.0",
    "semantic-release": "^17.4.7",
    "semantic-release-gitmoji": "^1.4.2",
    "standard-version": "^9.3.2",
    "typescript": "^4.5.4"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "lint-staged": {
    ".{ts,js,vue}": [
      "npm run lint",
      "git add ."
